  "version": "0.2.0",
  "configurations": [
  {
    "name": "Deno",
    "type": "pwa-node",
    "request": "launch",
    "runtimeExecutable": "deno",
    "runtimeArgs": ["run", "--inspect-brk", "-A", "index.ts"],
    "cwd": "${workspaceFolder}/leetcode/ts",
    "outputCapture": "std",
    "attachSimplePort": 9229
  },
  {
    "name": "JS test",
    "type": "node",
    "request": "launch",
    "runtimeExecutable": "node",
    "runtimeArgs": ["--inspect-brk", "app.mjs"],
    "cwd": "${workspaceFolder}/leetcode/js",
    "outputCapture": "std",
    "port": 9229
  },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'other'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=other"
        ],
        "filter": {
          "name": "other",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
  ]
}
